@model Lipar.Web.Areas.Admin.Models.Application.ProductModel

<div class="form-group">
    <label asp-for="MetaKeywords" class="control-label">@T("Product.Field.MetaKeywords")</label>
    <input type="text" class="form-control" asp-for="MetaKeywords">
    <span class="text-danger" asp-validation-for="MetaKeywords" />
</div>
<div class="form-group">
    <label asp-for="MetaDescription" class="control-label">@T("Product.Field.MetaDescription")</label>
    <input type="text" class="form-control" asp-for="MetaDescription">
    <span class="text-danger" asp-validation-for="MetaDescription" />
</div>
<div class="form-group">
    <label asp-for="MetaTitle" class="control-label">@T("Product.Field.MetaTitle")</label>
    <input type="text" class="form-control" asp-for="MetaTitle">
    <span class="text-danger" asp-validation-for="MetaTitle" />
</div>